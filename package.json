{
  "name": "n8n-nodes-imap",
  "version": "2.8.0",
  "description": "This node allows you to connect to an IMAP server and retrieve emails.",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Vasily Maslyukov",
    "email": "auro.coding@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/umanamente/n8n-nodes-imap.git"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ImapCredentials.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Imap/Imap.node.js"
    ]
  },
  "devDependencies": {
    "@types/imapflow": "^1.0.19",
    "@types/mailparser": "^3.4.4",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "n8n-workflow": "^1.82.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "imapflow": "^1.0.169",
    "mailparser": "^3.7.1",
    "nodemailer": "^6.10.0"
  }
}
